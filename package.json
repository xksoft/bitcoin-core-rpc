{
  "name": "bitcoin-core-rpc",
  "version": "1.0.0",
  "description": "\"bitcoin-core-rpc",
  "main": "app.js",
  "scripts": {
    "start": "tsc&node app.js"
  },
  "author": "xksoft",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": false,
      "node": true
    },
    "rules": {
      "quotes": [
        1,
        "single"
      ],
      "no-inner-declarations": [
        1,
        "both"
      ],
      "no-extra-boolean-cast": 1,
      "no-extra-semi": 1,
      "no-extra-parens": 1,
      "no-empty": 1,
      "no-use-before-define": [
        1,
        "nofunc"
      ],
      "complexity": [
        1,
        10
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "no-debugger": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty-character-class": 2,
      "no-invalid-regexp": 2,
      "no-func-assign": 2,
      "valid-typeof": 2,
      "no-unreachable": 2,
      "no-unexpected-multiline": 2,
      "no-sparse-arrays": 2,
      "no-shadow-restricted-names": 2,
      "no-undef": 2,
      "no-unused-vars": 2,
      "no-cond-assign": 2,
      "no-native-reassign": 2,
      "no-else-return": 1,
      "no-multi-spaces": 1,
      "block-scoped-var": 2,
      "dot-location": [
        2,
        "property"
      ],
      "no-lone-blocks": 2,
      "no-extend-native": 2,
      "no-floating-decimal": 2,
      "no-loop-func": 2,
      "no-new-func": 2,
      "no-self-compare": 2,
      "no-sequences": 2,
      "no-useless-concat": 2,
      "no-useless-call": 2,
      "curly": 2
    }
  }
}
